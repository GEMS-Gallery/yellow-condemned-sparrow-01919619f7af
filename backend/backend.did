type UserProfile = record {username: text;};
type Tweet = 
 record {
   author: principal;
   content: text;
   id: text;
   timestamp: Time;
 };
type Time = int;
type Result = 
 variant {
   err: text;
   ok: Tweet;
 };
service : {
  createTweet: (text) -> (Result);
  getTimeline: () -> (vec Tweet) query;
  getUserProfile: (principal) -> (opt UserProfile) query;
  updateUserProfile: (text) -> ();
}
