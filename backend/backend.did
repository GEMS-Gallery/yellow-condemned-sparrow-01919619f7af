type UserProfile = record {username: text;};
type Time = int;
type Result = 
 variant {
   err: text;
   ok: Msg;
 };
type Msg = 
 record {
   author: principal;
   content: text;
   id: text;
   likes: nat;
   replies: vec text;
   shares: nat;
   timestamp: Time;
 };
service : {
  createMsg: (text) -> (Result);
  getTimeline: () -> (vec Msg) query;
  getUserProfile: (principal) -> (opt UserProfile) query;
  likeMsg: (text) -> (Result);
  replyToMsg: (text, text) -> (Result);
  shareMsg: (text) -> (Result);
  updateUserProfile: (text) -> ();
}
